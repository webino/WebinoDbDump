    "type": "library",
    "keywords": [
        "database", "db", "dump", "webino", "zf2", "zend", "framework"
    ],
    "homepage": "http://github.com/webino/WebinoDbDump",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Webino, s. r. o.",
            "email": "info@webino.sk",
            "homepage": "http://webino.sk",
            "role": "Developer"
        },
        {
            "name": "Peter Bačinský",
